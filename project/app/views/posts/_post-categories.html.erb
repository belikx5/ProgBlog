<div class="main-page-aside">
        <h4 class="categories-header">Categories</h4>
        <nav class="main-page-side-navigation">
        <% arr = Array.new %>
        <% @posts.each do |p| %>
           <% p.tag_list.each do |t|
                if !arr.include?(t)
                arr.push(t)
            %>
                <span><%= link_to t, tagged_path(tag: t) %></span>
                <% end %>
              <% end %>
            <% end %>
        </nav>
      </div>